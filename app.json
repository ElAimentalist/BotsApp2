        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.akshit_tyagi7.topchat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2aba717a-7165-427c-9929-770582a070c9"
      }
    },
    "owner": "akshit_tyagi7"
  }
